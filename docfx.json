{
  "metadata": [
  ],
  "build": {
    "content": [
      {
        "files": [
          "pages/**/*.md",
          "pages/**/*.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "assets/**",
          "pages/**/media/**"
        ]
      }
    ],
    "overwrite": [
    ],
    "dest": "_site",
	"globalMetadata": {
		"_appTitle": "ezEngine",
		"_appLogoPath": "assets/images/EZ-text.svg",
		"_appFaviconPath": "assets/images/favicon.svg",
		"_enableSearch": true,
		"_enableGoogleAnalytics": true,
		"_enableNewTab": true
	},
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
	  "template"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}